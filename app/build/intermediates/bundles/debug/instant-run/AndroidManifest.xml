<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luis.gamestats"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.luis.gamestats.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.luis.gamestats.Title" />
        <activity
            android:name="com.example.luis.gamestats.BActivity"
            android:label="@string/title_activity_b" />
        <activity
            android:name="com.example.luis.gamestats.Basketball"
            android:label="@string/title_activity_basketball" />
        <activity
            android:name="com.example.luis.gamestats.Rules"
            android:label="@string/title_activity_rules" />
        <activity
            android:name="com.example.luis.gamestats.Players"
            android:label="@string/title_activity_players" />
        <activity
            android:name="com.example.luis.gamestats.TeamList"
            android:label="@string/title_activity_team_list" />
    </application>

</manifest>